<link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
<%= link_to image_tag('logo.jpg', class: 'img-responsive logo-admin', alt: ''), root_path, data: {turbolinks:"false"} %>
<div class="login-block">
    <h1>Login</h1>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= f.email_field :email, autofocus: true, placeholder: 'E-mail', id: 'username' %>
    <%= f.password_field :password, autocomplete: "off", placeholder: 'Senha', id: 'password' %>
    <button type="submit" name="button">Entrar</button>
    <% if devise_mapping.rememberable? -%>
    <div class="checkbox">
      <label>
        <%= f.check_box :remember_me %> Lembrar-me
      </label>
    </div>
    <% end -%>
    <% end %>
    <!-- <%= render "devise/shared/links" %> -->
  </div>
<!--
    <input type="text" value="" placeholder="Username" id="username" />
    <input type="password" value="" placeholder="Password" id="password" />
    <button>Submit</button> -->
</div>

